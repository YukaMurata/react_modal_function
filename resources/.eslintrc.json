{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "velocity": true,
    "YT": true
  },
  "rules": {
    "no-console": 0,
    "no-alert": 0,
    "no-debugger": 1,
    "no-async-promise-executor": 0,
    "no-misleading-character-class": 0,
    "no-useless-catch": 0,
    "require-atomic-updates": 0,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "bracketSpacing": true,
        "printWidth": 300
      }
    ]
  }
}
